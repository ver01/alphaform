{
    "name": "@ver01/form",
    "version": "1.0.7-alpha.1",
    "description": "make form just using schema",
    "keywords": [
        "ver01.com",
        "react form",
        "json schema"
    ],
    "scripts": {
        "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js",
        "build:readme": "toctoc README.md -w",
        "lint": "eslint src",
        "cs-check": "prettier 'src/**/*.js' --check",
        "cs-format": "prettier 'src/**/*.js' --write",
        "precommit": "npm run lint && npm run cs-format && git add .",
        "publish-to-npm": "npm run build:readme && npm run build:dist && npm publish",
        "preversion": "npm run build:readme && npm run build:dist && npm run cs-check && npm run lint"
    },
    "main": "dist/index.js",
    "dependencies": {
        "@ver01/form-theme-native": "^0.0.1",
        "@babel/runtime": "*",
        "lodash": "^4.17.15"
    },
    "peerDependencies": {
        "react": "*"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.4",
        "@babel/core": "^7.7.0",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-lodash": "^3.3.4",
        "cross-env": "^6.0.3",
        "eslint": "^6.7.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.17.0",
        "lodash-webpack-plugin": "^0.11.5",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.0",
        "toctoc": "^0.3.2",
        "webpack": "^4.41.0",
        "webpack-cli": "^3.3.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ver01/form.git"
    },
    "author": "Jerry<alphaformbase@gmail.com>",
    "license": "MIT",
    "homepage": "https://github.com/ver01/form#readme"
}
